<!--Site Footer Here-->
<footer id="site-footer" class="position-relative bg-red padding_top">
  <div class="svg-footer-top-holder">
    <div class="svg-footer-top"></div>
  </div>
  <div class="container">
    <div class="row pb-5">
      <div class="col-lg-6 col-md-7 col-sm-12">
        <div class="heading-title bottom30 wow fadeInLeft" data-wow-delay="200ms">
          <h2 class="text-light bottom30 darkcolor font-normal text-center text-md-left">
            <b>Cafenux</b>
          </h2>
          <h3 class="text-light bottom30 darkcolor font-normal text-center text-md-left">
            <b><%= t("footer.addressLabel") %>:</b> <%= t("footer.address") %>
          </h3>
        </div>
      </div>
      <div class="col-lg-6 col-md-5 col-sm-12 wow fadeInRight" data-wow-delay="200ms">
        <h2 class="text-light bottom30 darkcolor font-normal text-center text-md-left">
          <b><%= t("footer.contact") %></b>
        </h2>
        <h3 class="text-light bottom30 darkcolor font-normal text-center text-md-left">
          <b><%= t("footer.emailLabel") %>:</b> <%= t("footer.email") %>
        </h3>
        <h3 class="text-light bottom30 darkcolor font-normal text-center text-md-left">
          <b><%= t("footer.phoneLabel") %>:</b> <%= t("footer.phone") %>
        </h3>
      </div>
    </div>
  </div>
</footer>

<!--Footer ends-->
